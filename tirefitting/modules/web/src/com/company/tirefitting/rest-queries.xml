<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<queries xmlns="http://schemas.haulmont.com/cuba/rest-queries.xsd">
    <query name="requestByServicePoint" entity="tirefitting_Request" view="request">
        <jpql>
            <![CDATA[select r from tirefitting_Request r where r.servicePoint.id = :id]]>
        </jpql>
        <params>
            <param name="id" type="java.util.UUID"/>
        </params>
    </query>
</queries>
